{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "no-console": false, //Disable for typescript
    "jsx-no-lambda": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "src/assets/js/*.js",
      "src/assets/bootstrap/js/*.js",
      "node_modules/@types/bootstrap-select/*.d.ts"
    ]
  }
}
